<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" style="@style/id8_gs_edit_main_card_container_left_padding">
    <RelativeLayout style="@style/id8_gs_main_big_card_size">
        <LinearLayout android:orientation="vertical" android:id="@+id/ll_container" android:background="@drawable/gs_id8_main_icon_3app" android:layout_width="@dimen/id8_gs_big_card_layout_width" android:layout_height="@dimen/id8_gs_card_layout_height">
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:id="@+id/tv_name" style="@style/id8_gs_edit_card_title"/>
            </LinearLayout>
            <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
                <ImageView android:id="@+id/iv" android:layout_width="98dp" android:layout_height="98dp" android:layout_marginTop="80dp" android:layout_centerHorizontal="true"/>
            </RelativeLayout>
        </LinearLayout>
        <ImageView android:id="@+id/gs_id8_icon_edit_bg" android:src="@drawable/gs_id8_main_icon_edit_bg" style="@style/id8_gs_main_big_card_size"/>
        <ImageView android:id="@+id/remove" android:layout_width="50dp" android:layout_height="50dp" android:src="@drawable/bmw_id8_gs_main_edit_icon_delete" android:layout_marginStart="5dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
</RelativeLayout>
