<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/ll_container_gs" android:tag="layout/fragment_weather_gs_0" style="@style/id8_gs_main_card_container">
    <ImageView android:tag="binding_1" style="@style/id8_gs_main_big_card_size"/>
