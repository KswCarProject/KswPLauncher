<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/ll_container" style="@style/id8_main_card_container">
    <ImageView android:id="@+id/iv_3rd_bg" style="@style/id8_main_small_card_size"/>
    <ImageView android:id="@+id/iv_mask" style="@style/id8_main_small_card_size"/>
    <LinearLayout style="@style/id8_main_small_card_container">
        <TextView android:textColor="#ffffff" android:id="@+id/tv_name" style="@style/id8_main_card_title"/>
        <ImageView android:id="@+id/iv_3rd_divider" style="@style/id8_main_card_divider"/>
        <TextView android:textColor="#ffffff" android:id="@+id/tv_3rd_desc" android:text="@string/ksw_id8_trd_desc" style="@style/id8_main_card_desc"/>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageView android:id="@+id/iv" android:layout_width="125dp" android:layout_height="125dp" android:layout_marginTop="80dp" android:layout_centerHorizontal="true"/>
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
