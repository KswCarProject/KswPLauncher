<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:tag="layout/fragment_video_edit_0" style="@style/id8_edit_main_card_container_left_padding">
    <RelativeLayout android:id="@+id/layout" android:layout_width="@dimen/id8_small_card_layout_width_edit" android:layout_height="@dimen/id8_card_layout_height_edit">
        <FrameLayout android:background="@drawable/id8_main_edit_icon_video_bg" android:layout_width="@dimen/id8_small_card_layout_width_edit" android:layout_height="@dimen/id8_card_layout_height_edit">
            <ImageView android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/id8_main_edit_icon_video"/>
        </FrameLayout>
        <LinearLayout android:orientation="vertical" android:layout_width="@dimen/id8_small_card_layout_width_edit" android:layout_height="@dimen/id8_card_layout_height_edit">
            <TextView android:text="@string/ksw_id8_hd_video" style="@style/id8_main_card_title_edit"/>
            <ImageView android:background="@drawable/id8_main_edit_icon_weather_line" style="@style/id8_main_card_divider"/>
            <TextView android:textSize="13sp" android:textColor="@color/id8_font_warning" android:text="@string/ksw_id8_video_desc" style="@style/id8_main_card_desc_edit"/>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
