<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:tag="layout/activity_audi_mib3_sound_android_0" android:layout_width="match_parent" android:layout_height="match_parent">
    <android.support.v7.widget.AppCompatTextView android:textSize="21sp" android:textColor="@android:color/white" android:gravity="center_vertical" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="63dp" android:text="@string/audi_set_sound_hz_ms" android:drawablePadding="17dp" android:drawableStart="@drawable/audi_mib3_settings_icon_audio" android:paddingStart="19dp"/>
    <View android:id="@+id/title_divider" android:background="@drawable/audi_mib3_settings_list_line" android:layout_width="722dp" android:layout_height="1dp" android:layout_below="@+id/title"/>
    <ScrollView android:id="@+id/sv_sound" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/title_divider">
        <android.support.constraint.ConstraintLayout android:id="@+id/linearLayout4" android:layout_width="722dp" android:layout_height="match_parent">
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/hzMediaLinearLayout" android:layout_width="match_parent" android:layout_height="63dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0">
                <TextView android:id="@+id/audio_seekbar_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="90dp" android:text="@string/audi_set_sound_media_volume" android:layout_weight="1" style="@style/audi_set_default_text"/>
                <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/audi_mib3_settings_icon_sound" android:layout_marginStart="28dp"/>
                <SeekBar android:id="@+id/audio_seekbar" android:tag="binding_1" android:layout_width="366dp" android:layout_height="13dp" android:max="40" android:progressDrawable="@drawable/audi_mib3_seekbar_progress" android:thumb="@drawable/audi_mib3_settings_progress_bar_slider"/>
                <TextView android:gravity="center" android:id="@+id/audio_seekbar_right_text" android:tag="binding_2" android:layout_width="50dp" android:layout_height="wrap_content" android:layout_marginRight="16dp" style="@style/audi_mib3_set_default_text"/>
            </LinearLayout>
            <View android:background="@drawable/audi_mib3_settings_list_line" android:layout_width="0dp" android:layout_height="1dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/hzMediaLinearLayout"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/hzCallLinearLayout" android:layout_width="match_parent" android:layout_height="63dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/hzMediaLinearLayout">
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="90dp" android:text="@string/audi_set_sound_call_volume" android:layout_weight="1" style="@style/audi_set_default_text"/>
                <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/audi_mib3_settings_icon_sound" android:scaleType="centerInside" android:layout_marginStart="28dp"/>
                <SeekBar android:tag="binding_3" android:layout_width="366dp" android:layout_height="13dp" android:max="40" android:progressDrawable="@drawable/audi_mib3_seekbar_progress" android:thumb="@drawable/audi_mib3_settings_progress_bar_slider"/>
                <TextView android:gravity="center" android:tag="binding_4" android:layout_width="50dp" android:layout_height="wrap_content" android:layout_marginRight="16dp" style="@style/audi_mib3_set_default_text"/>
            </LinearLayout>
            <View android:background="@drawable/audi_mib3_settings_list_line" android:layout_width="0dp" android:layout_height="1dp" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/hzCallLinearLayout"/>
