<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AudiHomeItemWidthHeight" parent="">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="audi_set_checkbox" parent="@style/audi_set_default_text">
        <item name="android:background">@drawable/audi_set_item_selector</item>
        <item name="android:paddingLeft">45dp</item>
        <item name="android:paddingRight">70dp</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:button">@null</item>
    </style>
    <style name="audi_set_default_text" parent="">
        <item name="android:textSize">27sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="audi_set_info_item" parent="@style/audi_set_default_text">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/audi_set_item_selector</item>
        <item name="android:paddingLeft">28dp</item>
    </style>
    <style name="audi_set_item" parent="@style/audi_set_default_text">
        <item name="android:textColor">@color/audi_item_text_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/audi_set_item_selector</item>
        <item name="android:paddingLeft">28dp</item>
        <item name="android:paddingRight">70dp</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:drawableRight">@drawable/audi_set_item_icon_selector</item>
    </style>
</resources>
