<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Animation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>
    <style name="AppTheme">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowAnimationStyle">@style/Animation</item>
        <item name="colorAccent">@android:color/holo_blue_dark</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>
    <style name="AppThemeTran">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/Animation</item>
        <item name="colorAccent">@android:color/holo_blue_dark</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>
    <style name="AudiAppTheme">
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="AudiHomeItemWidthHeight">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="AudiText">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:padding">3dp</item>
        <item name="android:duplicateParentState">true</item>
        <item name="android:layout_marginLeft">-15dp</item>
        <item name="android:maxWidth">180dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="BaseActivityTheme">
        <item name="android:excludeFromRecents">true</item>
    </style>
    <style name="DayAppTheme">
        <item name="android:windowBackground">@android:color/white</item>
    </style>
    <style name="ad_eq_bt">
        <item name="android:background">@drawable/audi_eq_btn_selector</item>
    </style>
    <style name="audi_set_checkbox">
        <item name="android:background">@drawable/audi_set_item_selector</item>
        <item name="android:paddingLeft">45dp</item>
        <item name="android:paddingRight">80dp</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:button">@null</item>
    </style>
    <style name="audi_set_default_text">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="audi_set_info_item">
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:background">@drawable/audi_set_item_selector</item>
        <item name="android:paddingLeft">45dp</item>
        <item name="android:paddingRight">80dp</item>
    </style>
    <style name="audi_set_item">
        <item name="android:textColor">@color/audi_item_text_color</item>
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:background">@drawable/audi_set_item_selector</item>
        <item name="android:paddingLeft">45dp</item>
        <item name="android:paddingRight">80dp</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:drawableRight">@drawable/audi_set_item_icon_selector</item>
    </style>
</resources>
